    assert.equal(inscribe([(10, 250), (60, 300), (300, 60), (250, 10)]), 24000.0);
    assert.equal(inscribe([(10, 250), (60, 300), (110, 250), (160, 300), (210, 250), (160, 200), (300, 60), (250, 10)]), 48000.0);
