    assert.equal(inscribe([(6, 5), (10, 7), (8, 2)]), 20.0);
    assert.equal(inscribe([(2, 3), (3, 8), (8, 7), (9, 2), (3, 2), (4, 4), (6, 6), (7, 3), (5, 3)]), 41.538);
